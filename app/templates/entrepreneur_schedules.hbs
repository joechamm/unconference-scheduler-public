<div class="container" id="schedule-main">
  <div id="company-info">
    <h2 id="company-name">{{ name }}</h2>
  </div>
  <div id="company-schedule-container" class="table-responsive">
    <h3 class="sub-header">Meetings Schedule</h3>
    <table id="schedule-table" class="table table-striped table-bordered">
      <thead id="schedule-table-head">
        <tr id="schedule-row-head">
          <th>Time</th>
          <th>Company Name</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody id="schedule-table-body">
        {{#each meetings}}
          <tr>
            <td>{{ time }}</td>
            <td>{{ company }}</td>
            <td>{{ location }}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
